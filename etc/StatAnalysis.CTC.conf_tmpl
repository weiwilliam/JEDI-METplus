#
[dir]
INPUT_BASE = @INPUT_BASE@
OUTPUT_BASE = @OUTPUT_BASE@
PARM_BASE = @PARM_BASE@

[config]
PROCESS_LIST = StatAnalysis
LOOP_BY = VALID
VALID_TIME_FMT = %Y%m%d%H
VALID_BEG=@valid_begin@
VALID_END=@valid_end@
VALID_INCREMENT = @valid_inc@
LEAD_SEQ = @leadtime@
# LOG_LEVEL = DEBUG

STAT_ANALYSIS_RUNTIME_FREQ = RUN_ONCE_PER_INIT_OR_VALID

###
# File I/O
# https://metplus.readthedocs.io/en/latest/Users_Guide/systemconfiguration.html#directory-and-filename-template-info
###

STAT_ANALYSIS_OUTPUT_DIR = {OUTPUT_BASE}/../Data/output/stats
STAT_ANALYSIS_OUTPUT_TEMPLATE = f{lead_hour}/CTC.@varname@.{valid?fmt=%Y%m%d%H}.out

###
# StatAnalysis Settings
# https://metplus.readthedocs.io/en/latest/Users_Guide/wrappers.html#statanalysis
###

MODEL1 = NA
MODEL1_OBTYPE = NA
MODEL1_STAT_ANALYSIS_LOOKIN_DIR = python @embedded_py@ @embedded_input@ '@iodavarname@' '@mask_by_str@'
MODEL1_STAT_ANALYSIS_DUMP_ROW_TEMPLATE = f{lead_hour}/@varname@.dump.out

# PM2.5 EPA regualtion Good: 0-9, Moderate: 9-35, Unhealthy: >35
STAT_ANALYSIS_JOB1 = -line_type MPR -job aggregate_stat -dump_row [dump_row_file] -out_line_type CTC -by FCST_VAR -out_cnt_logic UNION -out_thresh "@OUTTHRESH@"
#">0&&<=9.,>9&&<=35,>35"
# O3 EPA regulation, 70 ppb, which is 7e-8
#STAT_ANALYSIS_JOB1 = -job aggregate_stat -dump_row [dump_row_file] -out_line_type FHO,CTC,CTS,CNT -by FCST_VAR -out_cnt_logic UNION -out_thresh ">0&&<=7e-8,>7e-8"


